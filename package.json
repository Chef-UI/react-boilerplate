{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate to start React project quickly.",
  "main": "index.js",
  "scripts": {
    "run:dev": "webpack-dev-server --host 0.0.0.0 --port 9090 --mode development --open",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "ngrok": "ngrok http 9090 -host-header='localhost:9090'",
    "lint": "prettier --write '**/*.js' '**/*.jsx' '**/*.scss' '**/*.html'",
    "build": "webpack --config webpack.config.js --mode production"
  },
  "repository": "https://github.com/Chef-UI/react-boilerplate.git",
  "author": "cl4pper <cl4pper@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "sass": "^1.48.0",
    "sass-loader": "10.1.1",
    "style-loader": "3.3.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  }
}
